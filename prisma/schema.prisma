// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Profile {
  id Int @id @default(autoincrement())
  email String  @unique
  name  String
  description String
  cv_link String
}

model Blogs {
  id Int @id @default(autoincrement())
  date_posted DateTime
  title String
  slug  String @unique
  description String?
}

